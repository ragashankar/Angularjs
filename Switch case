What is your fav character dialogue from Game of Thrones ?

var charac = prompt("What's your favorite character dialogue?","Type your favorite dialogue here.");

switch(charac) {
  case 'Jon Snow':
    console.log("Winter is coming!");
    break;
  case 'Tywrin':
    console.log("Lannisters always pays his debts!");
    break;
  case 'Daenerys':
    console.log("My reign has just begun!");
    break;
  default:
  console.log("Sorry, what's that?...I don't watch Game of Thrones");
  
}
