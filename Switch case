What do u think about the courses ? 

// JavaScript Document
var course = prompt("What do you think about the below course?");
switch(course){
case 'DS':
    console.log("Oh The"+" "+course+" course was basics of Data Science");
    break;
case 'BA':
    console.log("The"+" "+course+" course was business-oriented");
    break;
case 'ST':
    console.log("It's more of Adv statistics and R programming");
    break;
default:
    console.log("Oops! I have no idea what you're talking about");
}

