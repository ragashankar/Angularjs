var understand = true;

while( understand ){
	console.log("I'm learning while loops!");
	understand = false;
}

------------------------------------------------------------------------------
var coinFace = Math.floor(Math.random() * 2); // coinface is rounded to nearest integer of 0 or 1

while(coinFace===0)
{
console.log("Heads flipping!");
}
console.log("Done!Tails flipping");
